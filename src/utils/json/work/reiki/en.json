        {
            "layout": "key_improve",
            "title": "Key Improvements",
            "key_improve": [
                {
                    "bg_color": "transparent",
                    "title": "Lack of Memory",
                    "description": "The chemotherapy process messes up with the patients memory efficiency known as the Chemo brain effect which makes it hard for them to keep track of their pill and treatment schedule.",
                    "is_image": true,
                    "img": "/assets/img/work/reiki/key-improve/mellow-brain.png",
                    "img_alt": "Lack of Memory",
                    "width": 72
                },
                {
                    "bg_color": "transparent",
                    "title": "Mental Health",
                    "description": "Cancer patients are subjected to develop more mental health related issues like anxiety,stress and trauma due to the ill effects of the treatment which affects the quality of their life.",
                    "is_image": true,
                    "img": "/assets/img/work/reiki/key-improve/sad.png",
                    "img_alt": "Mental Health",
                    "width": 74
                },
                {
                    "bg_color": "transparent",
                    "title": "Lack of credible Information",
                    "description": "Too much influences and misconceptions about cancer and it’s diagnosis panics the users which makes the disease more frightening and mysterious to them.",
                    "is_image": true,
                    "img": "/assets/img/work/reiki/key-improve/question.png",
                    "img_alt": "Lack of credible Information",
                    "width": 42
                }
            ]
        },
