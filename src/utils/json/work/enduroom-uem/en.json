    "pageData": [
        {
            "layout": "project_intro",
            "project_intro": {
                "title": "UEM Security app",
                "description": "This case study focuses on the redesign and enhancement of a Unified Endpoint Management (UEM) application to address critical flaws in its current user interface (UI) and user flows. The goal is to create a more intuitive, efficient, and user-friendly experience for IT administrators, improving their ability to manage and secure a diverse range of endpoints effectively.",
                "img": "/assets/img/work/enduroom-uem/enduroom.jpg",
                "img_alt": "Enduroom",
                "history": [
                    {
                        "label": "Role",
                        "value": "Product Designer"
                    },
                    {
                        "label": "Industry",
                        "value": "Security"
                    },
                    {
                        "label": "Year",
                        "value": "2024"
                    }
                ],
                "details": [
                    {
                        "title": "What is a UEM? And Why it is required?",
                        "description": "Unified Endpoint Management (UEM) is a solution that allows organizations to manage and secure all devices, including desktops, laptops, smartphones, and tablets, from a single platform. UEM is essential for ensuring consistent security policies, simplifying IT management, supporting BYOD environments, and providing a unified approach to managing diverse endpoints, ultimately improving operational efficiency and security."
                    },
                    {
                        "title": "Context",
                        "description": "Users of the existing UEM solution have faced challenges such as unclear onboarding, limited customization, limited reports creations, complex navigation, accessibility issues, inability to input custom configuration values, lack of dark mode, cluttered UI, complicated user flows, inefficient navigation, and inconsistent experiences across platforms. These problems have resulted in decreased productivity and increased frustration. The redesigned application aims to resolve these issues by offering a cleaner, more focused user experience, utilizing data insights to empower IT administrators, while maintaining a consistent and minimalist design."
                    }
                ]
            }
        },
