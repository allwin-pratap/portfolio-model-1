            "project_intro": {
                "title": "Remoto",
                "description": "Remoto is a Remote assist application integrated as a part of the UEM software which allows the IT administrators to effortlessly view, control and troubleshoot devices decreasing downtime and enhancing productivity.",
                "img": "/assets/img/common/enduroom.jpg",
                "img_alt": "Enduroom Design system",
                "history": [
                    {
                        "label": "Role",
                        "value": "Product Designer"
                    },
                    {
                        "label": "Industry",
                        "value": "Security"
                    },
                    {
                        "label": "Company",
                        "value": "Mitsogo"
                    },
                    {
                        "label": "Year",
                        "value": "2024"
                    }
                ],
                "details": [
                    {
                        "title": "Context",
                        "description": "Remote Assist and Control Applications enable IT teams to troubleshoot, manage, and support devices from any location, ensuring quick resolution of technical issues, enhancing security, and improving efficiency for distributed workforces. These tools are essential for maintaining seamless IT operations in remote and hybrid work environments."
                    }
                ]
            }
        },
